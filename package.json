	},
	"scripts": {
		"build": "gatsby build",
		"clean": "gatsby clean",
		"dev-gatsby": "gatsby develop",
		"dev-typescript": "tsc -w",
		"dev": "concurrently \"npm:dev-gatsby\" \"npm:dev-typescript\"",
